CREATE VIEW IF NOT EXISTS test.senior_xptoDirectLeaders AS
SELECT
  md5(concat(row_sequence(),
        lider.id))		             	AS skDirectLeaderCode,
  lider.id 							  	AS nkDirectLeaderCode,
  lider.nomeLiderDireto		 			AS name,
  lider.estruturaequipe					AS teamStructureCode,
  lider.equipenome						AS teamCode,
  lider.empresa 						AS companyName,
  lider.registro						AS employeeType,
  lider.idCadastroColaborador			AS registerCode,
  lider.tipocontrato 					AS contractType,
  lider.generoColaborador				AS gender,
  lider.pcd								AS pcd,
  lider.instrucao						AS education,
  lider.dataAdmissaoTempoDeEmpresa		AS hiringDate,
  lider.situacao						AS workSituation,
  lider.dataDesligamento				AS dismissalDate,
  lider.causasdesligamento				AS dismissalCause,
  lider.dataNascimento					AS birthdayDate,
  lider.localtrabalho					AS workplace,
  lider.cidade							AS city,
  lider.estado							AS state,
  lider.cpf        						AS cpf,
  lider.centroDeCusto					AS costCenterCode,
  lider.nomesetor						AS workArea,
  lider.siglaunidade					AS workUnity,
  lider.cargosemnivel					AS jobTitle,
  lider.nivelcargo						AS jobSeniority,
  lider.cargocomnivel					AS fullJobTitle,
  lider.perfilcargo						AS jobTitleProfile,
  lider.tipoCargo						AS typeJob,
  lider.categoriaCargo 					AS jobCategory
FROM staging.senior_lideresdiretos AS lider;
