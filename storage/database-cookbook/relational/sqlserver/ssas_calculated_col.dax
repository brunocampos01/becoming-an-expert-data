// fact_call

Data IDDATA := 
    DATEVALUE(
        MID(fact_call[IDDATA],
            1, 4) & "/" & 
        MID(fact_call[IDDATA],
            5, 2)  & "/" &
        MID(fact_call[IDDATA],
            7, 2)
    )

Ano IDDATA := 
    MID(fact_call[IDDATA], 1, 4)

Mês IDDATA := 
    MID(fact_call[IDDATA], 5, 2)

Data IDDATA := 
    DATEVALUE([Dia IDDATA] & "/" & [Mês IDDATA] & "/" & [Ano IDDATA])

Média Horas Apontadas por Apontamento :=
    IF(AND([Ano IDDATA] == YEAR(TODAY()), [Mês IDDATA] == MONTH(TODAY())), 
        fact_call[Horas Apontadas],
        0)

Qtde Apontamento :=
    1
